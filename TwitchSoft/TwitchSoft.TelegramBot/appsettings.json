{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs\\log.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 214748364,
          "restrictedToMinimumLevel": "Information",
          "retainedFileCountLimit": 10,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TwitchSoft.WebApp"
    }
  },
  "ConnectionStrings": {
    "TwitchDbContext": "Server=.;Database=TwitchDb;User Id=sa;Password=BigPassw0rd"
  },
  "ServiceBusSettings": {
    "Host": "localhost",
    "VirtualHost": "/",
    "Username": "",
    "Password": ""
  },
  "Twitch": {
    "BotSettings": {
    }
  },
  "AllowedHosts": "*"
}
